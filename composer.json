{
  "name": "bytic/controllers",
  "description": "Php Framework",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "homepage": "https://github.com/bytic/controllers",
  "require": {
    "php": "^7.2 || ^8.0",
    "bytic/cache": "^0.9 || ^1.0",
    "bytic/config": "^0.9 || ^1.0",
    "bytic/container": "^0.9 || ^1.0 || ^2.0",
    "bytic/helpers": "^0.9 || ^1.0",
    "bytic/http": "^0.9 || ^1.0",
    "bytic/inflector": "^0.9 || ^1.0",
    "bytic/utility": "^1.0",
    "symfony/framework-bundle": "^4.0 || ^5.0 || ^6.0",
    "symfony/security-core": "^3.0 || ^4.0 || ^5.0 || ^6.0"
  },
  "require-dev": {
    "bytic/dispatcher": "^0.9 || ^1.0",
    "bytic/phpqatools": "^1.0",
    "bytic/view": "^0.9 || ^1.0",
    "mockery/mockery": "^1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Nip\\Controllers\\": "src"
    },
    "classmap": [
      "./legacy"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\Controllers\\Tests\\": "tests/src",
      "Nip\\Controllers\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "bytic/phpqatools": true
    }
  }
}
